meta {
  name: update_category_not_found_error
  type: http
  seq: 5
}

put {
  url: {{apiUrl}}/api/v1/categories/nonexistent-category-id
  body: json
  auth: none
}


headers {

  Authorization: Bearer {{idToken}}
  Content-Type: application/json
}

body:json {
  {
    "categoryName": "Some Name"
  }
}

tests {
  test("Status is 404", function() {
    expect(res.status).to.equal(404);
  });
  

  test("Response has not found error message", function() {
    expect(res.body).to.have.property('error');
    expect(res.body.error).to.include('not found');
  });
}
